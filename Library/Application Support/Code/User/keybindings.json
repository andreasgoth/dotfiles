// Place your key bindings in this file to overwrite the defaults
[
  {
    "key": "ctrl+shift+cmd+[Minus]",
    "command": "workbench.action.zoomIn"
  },
  {
    "key": "cmd+[Minus]",
    "command": "-workbench.action.zoomIn"
  },
  {
    "key": "cmd+numpad_add",
    "command": "-workbench.action.zoomIn"
  },
  {
    "key": "ctrl+shift+cmd+-",
    "command": "workbench.action.zoomOut"
  },
  {
    "key": "cmd+-",
    "command": "-workbench.action.zoomOut"
  },
  {
    "key": "cmd+[Minus]",
    "command": "fontshortcuts.increaseFontSize",
    "when": "editorFocus"
  },
  {
    "key": "shift+cmd+0",
    "command": "-fontshortcuts.increaseFontSize",
    "when": "editorFocus"
  }
]
